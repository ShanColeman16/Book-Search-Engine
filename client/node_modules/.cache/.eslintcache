ges\\Book-Search-Engine\\client\\src\\App.js",["55"],"C:\\Users\\blkqu\\bootcamp\\challenges\\Book-Search-Engine\\client\\src\\components\\Navbar.js",[],"C:\\Users\\blkqu\\bootcamp\\challenges\\Book-Search-Engine\\client\\src\\pages\\SavedBooks.js",["56"],"import React from 'react';\nimport { Jumbotron, Container, CardColumns, Card, Button } from 'react-bootstrap';\nimport { useQuery, useMutation } from '@apollo/client';\nimport { REMOVE_BOOK } from '../utils/mutations';\nimport { GET_ME } from '../utils/queries';\nimport Auth from '../utils/auth';\nimport { removeBookId } from '../utils/localStorage';\n\nconst SavedBooks = () => {\n  const { loading, data } = useQuery(GET_ME);\n  const [removeBook ] = useMutation(REMOVE_BOOK);\n  const userData = data?.me || [];\n\n  // use this to determine if `useEffect()` hook needs to run again\n  // const userDataLength = Object.keys(userData).length;\n\n    const handleDeleteBook = async (bookId) => {\n      const token = Auth.loggedIn() ? Auth.getToken() : null;\n\n      if (!token) {\n        return false;\n      }\n    \n    try {\n      const { data } = await removeBook ({\n        variables: { bookId },\n      });\n        // upon success, remove book's id from localStorage\n      removeBookId(bookId);\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  // if data isn't here yet, say so\n  if (loading) {\n    return <h2>LOADING...</h2>;\n  }\n\n  return (\n    <>\n      <Jumbotron fluid className='text-light bg-dark'>\n        <Container>\n          <h1>Viewing saved books!</h1>\n        </Container>\n      </Jumbotron>\n      <Container>\n        <h2>\n          {userData.savedBooks.length\n            ? `Viewing ${userData.savedBooks.length} saved ${userData.savedBooks.length === 1 ? 'book' : 'books'}:`\n            : 'You have no saved books!'}\n        </h2>\n        <CardColumns>\n          {userData.savedBooks.map((book) => {\n            return (\n              <Card key={book.bookId} border='dark'>\n                {book.image ? <Card.Img src={book.image} alt={`The cover for ${book.title}`} variant='top' /> : null}\n                <Card.Body>\n                  <Card.Title>{book.title}</Card.Title>\n                  <p className='small'>Authors: {book.authors}</p>\n                  <Card.Text>{book.description}</Card.Text>\n                  <Button className='btn-block btn-danger' onClick={() => handleDeleteBook(book.bookId)}>\n                    Delete this Book!\n                  </Button>\n                </Card.Body>\n              </Card>\n            );\n          })}\n        </CardColumns>\n      </Container>\n    </>\n  );\n};\n\nexport default SavedBooks;\n","C:\\Users\\blkqu\\bootcamp\\challenges\\Book-Search-Engine\\client\\src\\pages\\SearchBooks.js",["57","58","59"],"C:\\Users\\blkqu\\bootcamp\\challenges\\Book-Search-Engine\\client\\src\\components\\LoginForm.js",["60"],"C:\\Users\\blkqu\\bootcamp\\challenges\\Book-Search-Engine\\client\\src\\components\\SignupForm.js",["61"],"C:\\Users\\blkqu\\bootcamp\\challenges\\Book-Search-Engine\\client\\src\\utils\\auth.js",["62"],"// use this to decode a token and get the user's information out of it\nimport decode from 'jwt-decode';\n\n// create a new class to instantiate for a user\nclass AuthService {\n  // get user data\n  getProfile() {\n    return decode(this.getToken());\n  }\n\n  // check if user's logged in\n  loggedIn() {\n    // Checks if there is a saved token and it's still valid\n    const token = this.getToken();\n    return !!token && !this.isTokenExpired(token); // handwaiving here\n  }\n\n  // check if token is expired\n  isTokenExpired(token) {\n    try {\n      const decoded = decode(token);\n      if (decoded.exp < Date.now() / 1000) {\n        return true;\n      } else return false;\n    } catch (err) {\n      return false;\n    }\n  }\n\n  getToken() {\n    // Retrieves the user token from localStorage\n    return localStorage.getItem('id_token');\n  }\n\n  login(idToken) {\n    // Saves user token to localStorage\n    localStorage.setItem('id_token', idToken);\n    window.location.assign('/');\n  }\n\n  logout() {\n    // Clear user token and profile data from localStorage\n    localStorage.removeItem('id_token');\n    // this will reload the page and reset the state of the application\n    window.location.assign('/');\n  }\n}\n\nexport default new AuthService();\n","C:\\Users\\blkqu\\bootcamp\\challenges\\Book-Search-Engine\\client\\src\\utils\\localStorage.js",[],"C:\\Users\\blkqu\\bootcamp\\challenges\\Book-Search-Engine\\client\\src\\utils\\API.js",[],"C:\\Users\\blkqu\\bootcamp\\challenges\\Book-Search-Engine\\client\\src\\utils\\mutations.js",[],"C:\\Users\\blkqu\\bootcamp\\challenges\\Book-Search-Engine\\client\\src\\utils\\queries.js",[],{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","severity":1,"message":"68","line":4,"column":8,"nodeType":"69","messageId":"70","endLine":4,"endColumn":18},{"ruleId":"67","severity":1,"message":"71","line":25,"column":15,"nodeType":"69","messageId":"70","endLine":25,"endColumn":19},{"ruleId":"67","severity":1,"message":"72","line":3,"column":10,"nodeType":"69","messageId":"70","endLine":3,"endColumn":21},{"ruleId":"67","severity":1,"message":"73","line":16,"column":24,"nodeType":"69","messageId":"70","endLine":16,"endColumn":39},{"ruleId":"67","severity":1,"message":"74","line":61,"column":11,"nodeType":"69","messageId":"70","endLine":61,"endColumn":21},{"ruleId":"67","severity":1,"message":"75","line":12,"column":19,"nodeType":"69","messageId":"70","endLine":12,"endColumn":24},{"ruleId":"67","severity":1,"message":"75","line":11,"column":21,"nodeType":"69","messageId":"70","endLine":11,"endColumn":26},{"ruleId":"76","severity":1,"message":"77","line":49,"column":1,"nodeType":"78","endLine":49,"endColumn":34},"no-native-reassign",["79"],"no-negated-in-lhs",["80"],"no-unused-vars","'SavedBooks' is defined but never used.","Identifier","unusedVar","'data' is a